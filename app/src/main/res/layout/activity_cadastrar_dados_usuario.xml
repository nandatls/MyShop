<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadastrarDadosUsuario">

    <TextView
        android:id="@+id/textview_telacadastrardadosusuario_cadastrardados"
        android:layout_width="341dp"
        android:layout_height="59dp"
        android:text="Cadastrar Dados Cliente"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.371"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />

    <Button
        android:id="@+id/btn_telacadastrardadosusuario_cadastrar"
        android:layout_width="185dp"
        android:layout_height="61dp"
        android:text="CADASTRAR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.887" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="358dp"
        android:layout_height="462dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/btn_telacadastrardadosusuario_cadastrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.551">

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_nomeusuario"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Nome"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_emailusuario"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Email"
            android:inputType="textEmailAddress" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_senhausuario"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Senha"
            android:inputType="numberPassword" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_docusuario"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Documento"
            android:inputType="numberSigned" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_telefoneusuario"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Telefone"
            android:inputType="phone" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_cep"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Cep"
            android:inputType="textPostalAddress" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_end"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="EndereÃ§o"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_bairro"
            android:layout_width="335dp"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Bairro"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_cidade"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Cidade"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/edttxt_telacadastrardadosusuario_estado"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:ems="10"
            android:hint="Estado"
            android:inputType="textPersonName" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>